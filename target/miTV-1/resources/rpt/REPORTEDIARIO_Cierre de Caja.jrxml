	<queryString>
		<![CDATA[SELECT
    
    cajas."fecha" AS cajas_fecha,
     cajas."cheque_usd" AS cajas_cheque_usd,
     cajas."cheque_gs" AS cajas_cheque_gs,
     cajas."cant_usd" AS cajas_cant_usd,
     cajas."cant_gs" AS cajas_cant_gs,
      cajas."estado" as estado,
     Usuario.username AS Username
     
    
 FROM
     "cajas" cajas 
inner join "usuarios"Usuario on cajas.usuario_id = Usuario.usuario_id
WHERE
     cajas."fecha"::date = $P{FECHA_HASTA} and cajas."estado" = false]]>
