	<queryString>
		<![CDATA[SELECT
     (SUM(Stock.ingreso)-SUM(stock.egreso)) AS Suma,
     Stock."stock_id" AS Stock_ID,
     Stock."id_producto" AS ID_Producto,
     Usuarios."username" AS Usuario,
     Producto."descripcion" AS Descripcion,
     Stock."ingreso" AS Ingreso,
     Stock."egreso" AS Egreso
FROM
     "stock" Stock INNER JOIN "usuarios" Usuarios ON Stock."usuario" = Usuarios."usuario_id"
     INNER JOIN "producto" Producto ON Stock."id_producto" = Producto."producto_id"
GROUP BY
     Stock.stock_id,
     Stock.id_producto,
     Usuarios.username,
     Producto.descripcion,
     Stock.ingreso,
     Stock.egreso]]>
	</queryString>
