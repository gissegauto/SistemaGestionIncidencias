	<queryString>
		<![CDATA[SELECT

     OTrabajo.observacion AS Observacion,
     OTrabajo.prioridad AS Prioridad,
     OTrabajo.fecha_inicio As Inicio,
     OTrabajo.fecha_fin AS Fin,
     OTrabajo.orden_nro As OrdenNro,
     cliente.nombre As Cliente,
     cliente.comentario AS Comentario,
     servicios.cabina AS Cabina,
     servicios.servicio AS Servicio,
     servicios.servicio_id AS ServicioID,
     OT.cantidad AS Cantidad,
     OT.personal_id AS Personal_ID,
     OT.cabina AS Cabina,
     OT.planificado AS Planificado,
     OT.observacion AS Observacion,
     OT.granallado AS Granallado,
     OT.hornogrande AS HornoGrande
FROM
     "public"."cliente" cliente 
     INNER JOIN "public"."ordenes_trabajo" OTrabajo ON cliente."cliente_id" = OTrabajo.cliente_id
     INNER JOIN "public".orden_trabajo_detalle OT ON OTrabajo."orden_id" = OT."orden_id"
     INNER JOIN  "public"."servicios" servicios ON OT."servicio_id" = servicios."servicio_id"
 WHERE   OTrabajo.fecha_inicio between $P{fecha_Desde} and $P{fecha_Hasta}]]>
	</queryString>
