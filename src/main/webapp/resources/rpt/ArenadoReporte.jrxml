	<queryString>
		<![CDATA[SELECT
     ordenes_trabajo."orden_id" AS ordenes_trabajo_orden_id,
     ordenes_trabajo."orden_nro" AS ordenes_trabajo_orden_nro,
     ordenes_trabajo."fecha" AS ordenes_trabajo_fecha,
     OT."total" AS ordenes_trabajo_total,
     ordenes_trabajo."cliente_id" AS ordenes_trabajo_cliente_id,
     
     cliente."cliente_id" AS cliente_cliente_id,
     cliente."nombre" AS cliente_nombre,
     servicios."servicio_id" AS servicios_servicio_id,
     servicios."servicio" AS servicios_servicio
FROM
     "public"."cliente" cliente INNER JOIN "public"."ordenes_trabajo" ordenes_trabajo ON cliente."cliente_id" = ordenes_trabajo."cliente_id"
     INNER JOIN "public".orden_trabajo_detalle OT ON ordenes_trabajo."orden_id" = OT."orden_id"
     INNER JOIN  "public"."servicios" servicios ON OT."servicio_id" = servicios."servicio_id"
     where servicios.servicio_id = 1 and  ordenes_trabajo."fecha" between $P{fecha_Desde} and $P{fecha_Hasta}]]>
	</queryString>
